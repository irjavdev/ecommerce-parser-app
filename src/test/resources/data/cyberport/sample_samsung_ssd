<script type="text/javascript" no_defer="">
/* <![CDATA[ */


function lazyLoadImages()
{
    var lazyImages = document.getElementsByClassName("lazyImage");
    var i = lazyImages.length;
    var loaded_images = 0;
    while(i--) {
        var elm = lazyImages[i];


        var old_src = elm.getAttribute("src");
        if (
            typeof(old_src) == "string"
            && old_src != ""
            && old_src != window.location.href // wenn src leer ist, kann auch die location drin stehen
        ) {
            continue;
        }

        loaded_images++;
        var dataSrc = elm.getAttribute("data-src").toString();
        var src = ""+dataSrc;
        elm.setAttribute("src", src);
    }
};

Event.observe(window, "load", lazyLoadImages);
