    function setShopTypeContext() {


        if (config.get('shopType') == 'b2b') {
            jQuery('#bestell_b2c,#header-shoptype-b2c,#b2c_Link').hide();
        } else {
            jQuery('#bestell_b2b,#header-shoptype-b2b,#b2b_Rabatt_Link').hide();
        }
    }


