<script type="text/javascript">
	function downloadAtOnload() {
		var nameElements;

		try {
			nameElements = document.getElementsByClassName('asyncLoad');
		} catch(e) {
			nameElements = document.querySelectorAll('.asyncLoad');
		}
		for(var i = 0; i < nameElements.length; i++) {
			if (nameElements[i].className.indexOf('jsname') !== -1) {
				var element = document.createElement("script");
				element.src = nameElements[i].href;
			} else {
				var element = document.createElement("link");
				element.href = nameElements[i].href;
				element.rel = 'stylesheet';
			}
			document.body.appendChild(element);
		}
	}
	if(window.addEventListener) {
		window.addEventListener("load", downloadAtOnload, false);
	} else if(window.attachEvent) {
		window.attachEvent("onload", downloadAtOnload);
	} else {
		window.onload = downloadAtOnload;
	}
</script>




