            $(document).ready(function() {
                if(/^(.*; ?)?conradPreferredDeviceType=b2c(;.+)?$/.test(document.cookie)) {
                    $("[data-toogle-to-mobile]").insertAfter(".footer").css("display", "block");

                    $(document).on("click", "[data-toogle-to-mobile]", function() {
                        document.cookie = "conradPreferredDeviceType" + '=; expires=Thu, 01 Jan 1970 00:00:01 GMT; path=/';
                        location.reload();
                    });
                }
            });
        })(jQuery);
    </script>
</div>



